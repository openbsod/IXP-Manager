<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Entities\PatchPanelPortHistoryFile" table="patch_panel_port_history_file" tableName="patch_panel_port_history_file">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" length="255" nullable="false"/>
    <field name="type" type="string" length="255" nullable="false"/>
    <field name="uploaded_at" type="datetime" nullable="false"/>
    <field name="uploaded_by" type="string" nullable="false"/>
    <field name="size" type="integer" nullable="false"/>
    <field name="is_private" type="boolean" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="storage_location" type="string" length="255" nullable="false"/>
    <many-to-one field="patchPanelPortHistory" target-entity="Entities\PatchPanelPortHistory" inversed-by="patchPanelPortHistoryFiles">
      <join-columns>
        <join-column name="patch_panel_port_history_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
